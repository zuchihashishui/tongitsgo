                <bitmap android:src="@drawable/anythink_myoffer_video_no_mute_v2" />
            </item>
        </layer-list>
    </item>
    <item>
        <layer-list>
            <item>
                <shape android:shape="oval">
                    <solid android:color="#4d000000" />
                </shape>
            </item>
            <item android:left="2.0dip" android:top="2.0dip" android:right="2.0dip" android:bottom="2.0dip">
